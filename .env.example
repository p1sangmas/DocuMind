# DocuMind Docker Environment Configuration

# Timezone - will be set automatically by the run_docker.sh script
TZ=UTC

# Enable debug mode for development
DEBUG=false

# Port configuration (if you change these, also update the docker-compose.yml file)
WEB_UI_PORT=8080
API_PORT=8000
OLLAMA_PORT=11434

# GPU configuration
# Set to "true" to explicitly enable GPU support 
# or "auto" to let the system detect GPU availability
ENABLE_GPU=auto
